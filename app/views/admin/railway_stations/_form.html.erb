<%= form_for([:admin, railway_station]) do |f| %>
  <%= render 'common/errors', resource: railway_station %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :arrival %>
    <%= date_field("f","arrival") %>
  </div>

  <div class="field">
    <%= f.label :departure %>
    <%= date_field("f","departure") %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
